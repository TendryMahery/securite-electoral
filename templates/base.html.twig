<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link
		rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{vite_asset("main.js")}}
			{{vite_asset("tailwind.js")}}

		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<style>
		.carousel-item {
			display: none;
		}
		.carousel-item.active {
			display: block;
		}
	</style>
    <body>
		<div class="relative w-full ">
			{% for message in app.flashes('success') %}
				<div class="absolute px-4 left-1/2	z-10 translate-x-2/4 mt-8 text-white py-2 bg-green-500 ">
					{{ message }}
				</div>
			{% endfor %}
			{% block body %}{% endblock %}
		</div>
    </body>
</html>
